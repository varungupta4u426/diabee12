      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="warp">
        <div class="form-group">
          <label for="email">Email address:</label>
          <%= f.email_field :email, autofocus: true,:class=>"form-control", :id=>"email" %>
        </div>

        <div class="form-group">
          <label for="pwd">Password:</label>
          <%= f.password_field :password, autocomplete: "off",:class=>"form-control", :id=>"pwd" %>
        </div>

        <% if devise_mapping.rememberable? -%>
          <div class="form-group">
          <div class="checkbox">
             <label><input type="checkbox" value="1" name="counsellor[remember_me]">Remember Password</label>
          </div>
          </div>
        <% end -%>

        <div class="actions">
          <%= f.submit "Submit" %>
        </div>
        <div class="form-group">
        <p><%= render "counsellor/shared/links" %></p>
        </div>
        <div>
      <% end %>
     



    </div>
